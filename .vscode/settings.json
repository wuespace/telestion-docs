{
	"conventionalCommits.scopes": [
		"readme",
		"ci",
		"vscode-config",
		"deployment",
		"templates",
		"stylesheet",
		"components",
		"docs/internal",
		"docs/client",
		"docs/application",
		"docs/general",
		"images"
	],
	"pasteImage.basePath": "${projectRoot}/static",
	"pasteImage.path": "${projectRoot}/static/img",
	"pasteImage.insertPattern": "<Image src=\"${imageSyntaxPrefix}${imageFilePath}${imageSyntaxSuffix}\" />"
}
